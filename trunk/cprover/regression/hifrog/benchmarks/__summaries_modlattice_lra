;;;;; Start of Mod rules
(declare-fun |_mod#0| (Real Real) Real)
;;;;;
;; C3 definition
(define-fun |mod_C3#0| (
    (|mod_C3::a!0| Real)
    (|mod_C3::n!0| Real)
    (|hifrog::fun_start| Bool)
    (|hifrog::fun_end| Bool) 
    (|mod_C3::#return_value!0| Real) )  Bool
  (and 
    (= |mod_C3::#return_value!0| (|_mod#0| |mod_C3::a!0| |mod_C3::n!0|))
    (or (not |hifrog::fun_end|) (and |hifrog::fun_start|
         (<= 0 |mod_C3::n!0|)
    ))
  )
)
;;;;;
;; C2 definition
(define-fun |mod_C2#0| (
    (|mod_C2::a!0| Real)
    (|mod_C2::n!0| Real)
    (|hifrog::fun_start| Bool)
    (|hifrog::fun_end| Bool) 
    (|mod_C2::#return_value!0| Real) )  Bool
  (and 
    (= |mod_C2::#return_value!0| (|_mod#0| |mod_C2::a!0| |mod_C2::n!0|))
    (or (not |hifrog::fun_end|) (and |hifrog::fun_start|
          (= |mod_C2::n!0| 0)
    ))
  )
)
;;;;;
;; n=1, n=2, n>2
; Fact a
(define-fun |mod_Ca#0| (
    (|mod_Ca::a!0| Real)
