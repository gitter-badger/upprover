    (|hifrog::fun_end| Bool)
    (|mod_Ca::#return_value!0| Real) )  Bool
  (let ((?def999 |mod_Ca::#return_value!0|))
  (let ((?def998 (= (|_mod#0| |mod_Ca::a!0| |mod_Ca::n!0|) ?def999)))
  (let ((?def997 (= ?def999 0)))
  (let ((?def996 (= |mod_Ca::n!0| 1)))
  (let ((?def995 (=> ?def996 (and ?def997 ?def998))))
  (let ((?def994 (or (not |hifrog::fun_end|) (and |hifrog::fun_start| ?def996))))
  (let ((?def900 (and ?def994 ?def995)))

?def900
))))))))
