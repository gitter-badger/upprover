; Fact b
(define-fun |mod_Cb#0| (
    (|mod_Cb::a!0| Real)
    (|mod_Cb::n!0| Real)
    (|hifrog::fun_start| Bool)
    (|hifrog::fun_end| Bool) 
    (|mod_Cb::#return_value!0| Real) )  Bool
  (let ((?def899 |mod_Cb::#return_value!0|))
  (let ((?def898 (= (|_mod#0| |mod_Cb::a!0| |mod_Cb::n!0|) ?def899)))
  (let ((?def897 (or (= ?def899 0) (= ?def899 1))))
  (let ((?def896 (= |mod_Cb::n!0| 2)))
  (let ((?def895 (=> ?def896 (and ?def897 ?def898))))
  (let ((?def894 (or (not |hifrog::fun_end|) (and |hifrog::fun_start| ?def896))))
  (let ((?def800 (and ?def894 ?def895)))

?def800
))))))))
