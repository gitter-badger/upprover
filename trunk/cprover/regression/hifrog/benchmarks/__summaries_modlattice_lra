    (|mod_Ck::#return_value!0| Real) )  Bool
  (let ((?def699 |mod_Ck::#return_value!0|))
  (let ((?def698 (= (|_mod#0| (+ (|_mod#0| |mod_Ck::a!0| |mod_Ck::n!0|)
			     (|_mod#0| (- |mod_Ck::a!0|) |mod_Ck::n!0|)
			              ) |mod_Ck::n!0|) ?def699)))
  (let ((?def697 (= ?def699 0)))
  (let ((?def696 (and ?def698 ?def697)))
  (let ((?def695 (or (not |hifrog::fun_end|) (and |hifrog::fun_start| ?def696))))

?def695
))))))
