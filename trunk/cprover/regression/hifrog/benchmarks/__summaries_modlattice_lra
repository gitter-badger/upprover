  (let ((?def397 (= ?def399 |mod_Cf::a!0|)))
  (let ((?def396 (and (<= 0 |mod_Cf::a!0|) (< 0 |mod_Cf::n!0|))))
  (let ((?def395 (< |mod_Cf::a!0| |mod_Cf::n!0|)))
  (let ((?def394 (=> (and ?def395 ?def396) (and ?def397 ?def398))))
  (let ((?def393 (or (not |hifrog::fun_end|) (and |hifrog::fun_start| (and ?def395 ?def396)))))
  (let ((?def300 (and ?def393 ?def394)))

